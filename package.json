{
    "name": "megaanticheat",
    "scripts": {
        "build-master": "rm -rf ./master/build && tsc -p ./master",
        "build-local": "rm -rf ./local/build && tsc -p ./local",
        "build-ui": "rm -rf ./ui/build && pnpm -C ui build",
        "lint": "eslint . --ext .ts",
        "fmt": "prettier --write . && pnpm -C ui fmt"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "eslint": "^8.42.0",
        "prettier": "^2.8.8"
    }
}
